{
	"easycom": {
		"^u-(.*)": "@/uview-ui/components/u-$1/u-$1.vue"
	},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "浏览列表"
			}
		}, {
			"path": "pages/myInfo/myInfo",
			"style": {
				"navigationBarTitleText": "个人中心"
			}
		}
		// , {
		// 	"path": "pages/uploadList/uploadList",
		// 	"style": {
		// 		"navigationBarTitleText": "新增列表"
		// 	}
		// }
		, {
			"path": "pages/details/details",
			"style": {
				"navigationBarTitleText": "详情"
			}
		}, {
			"path": "pages/myInfo/myList/myList",
			"style": {
				"navigationStyle": "custom" // 隐藏系统导航栏
				// "navigationBarTextStyle": "black" // 状态栏字体为白色，只能为 white-白色，black-黑色 二选一
				// "navigationBarTitleText": "我的列表"
			}
		}, {
			"path": "pages/myInfo/addItem/addItem",
			"style": {
				"navigationBarTitleText": "新增列表"
			}
		}, {
			"path": "pages/myInfo/contactUs/contactUs",
			"style": {}
		}
	    ,{
            "path" : "pages/myInfo/groupList/groupList",
            "style" : {"navigationStyle": "custom"}
        }
    ],
	"tabBar": {
		"color": "#808080",
		"selectedColor": "#ff713f",
		"list": [{
				"pagePath": "pages/index/index",
				"iconPath": "static/tab-cate.png",
				"selectedIconPath": "static/tab-cate-current.png",
				"text": "浏览列表"
			},
			// {
			// 	"pagePath": "pages/uploadList/uploadList",
			// 	"iconPath": "static/tab-cate.png",
			// 	"selectedIconPath": "static/tab-cate-current.png",
			// 	"text": "新增列表"
			// },
			{
				"pagePath": "pages/myInfo/myInfo",
				"iconPath": "static/tab-my.png",
				"selectedIconPath": "static/tab-my-current.png",
				"text": "个人中心"
			}
		]
	},
	"globalStyle": {
		// "navigationStyle":"custom",
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	}
}
